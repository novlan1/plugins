{
  "name": "plugin-light",
  "version": "1.5.26",
  "private": "true",
  "description": "Webpack 相关插件、Loader、基础配置及 CLI 命令",
  "keywords": [
    "uni-app",
    "press-ui",
    "plugin",
    "loader",
    "webpack",
    "plugin-light",
    "cli"
  ],
  "homepage": "https://novlan1.github.io/docs/plugin-light/",
  "bugs": {
    "url": "https://github.com/novlan1/plugin-light/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/novlan1/plugin-light"
  },
  "license": "MIT",
  "author": "novlan1",
  "files": [
    "README.md",
    "lib/",
    "bin/",
    "uni-h5/",
    "mp-plugin-public/",
    "public-script/"
  ],
  "scripts": {
    "check": "tsc --incremental --noEmit",
    "docs:build": "pnpm --filter=\"docs\" build",
    "docs:build:unity": "PUBLISH_PATH=/docs/plugin-light/ npm run docs:build",
    "docs:deploy": "node script/docs/deploy",
    "docs:deploy:mine": "node script/docs/docs-mine",
    "docs:deploy:unity": "node script/docs/deploy-unity",
    "docs:dev": "pnpm --filter=\"docs\" dev",
    "docs:gen": "node script/docs/gen-docs-monorepo",
    "init": "sh script/init/init.sh",
    "init:env": "node script/env/rainbow-env",
    "init:loader": "node script/docs/init-loader",
    "link": "node script/npm-link-simple/index",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue .",
    "md:doc": "node script/md-to-doc/md-to-doc.js",
    "prepare": "husky install",
    "release": "node script/monorepo/release-root.script.js master --needVersionTip --needExecInit",
    "release-major": "standard-version --release-as major -a",
    "release-minor": "standard-version --release-as minor -a",
    "release-patch": "standard-version --release-as patch -a",
    "release:first": "standard-version --first-release",
    "spider:area": "node script/spider-area/index",
    "test": "jest --coverage --verbose -u",
    "version:tip": "node script/version-tip",
    "write:npm:token": "node script/write-npm-token.js"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-3000020210521001",
